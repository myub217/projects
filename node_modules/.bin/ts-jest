#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_e459f019e557f3a6e12137b75ac12f68/node_modules/ts-jest/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_e459f019e557f3a6e12137b75ac12f68/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_e459f019e557f3a6e12137b75ac12f68/node_modules/ts-jest/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_e459f019e557f3a6e12137b75ac12f68/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_e459f019e557f3a6e12137b75ac12f68/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_e459f019e557f3a6e12137b75ac12f68/node_modules/ts-jest/cli.js" "$@"
fi
