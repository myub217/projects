#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-unused-exports@7.0.3_typescript@5.8.3/node_modules/ts-unused-exports/bin/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-unused-exports@7.0.3_typescript@5.8.3/node_modules/ts-unused-exports/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-unused-exports@7.0.3_typescript@5.8.3/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-unused-exports@7.0.3_typescript@5.8.3/node_modules/ts-unused-exports/bin/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-unused-exports@7.0.3_typescript@5.8.3/node_modules/ts-unused-exports/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/ts-unused-exports@7.0.3_typescript@5.8.3/node_modules:/data/data/com.termux/files/home/projects/projects1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-unused-exports/bin/ts-unused-exports" "$@"
else
  exec node  "$basedir/../ts-unused-exports/bin/ts-unused-exports" "$@"
fi
