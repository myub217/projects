#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/projects/node_modules/.pnpm/parse-github-url@1.0.3/node_modules/parse-github-url/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/parse-github-url@1.0.3/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/projects/node_modules/.pnpm/parse-github-url@1.0.3/node_modules/parse-github-url/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/parse-github-url@1.0.3/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../parse-github-url@1.0.3/node_modules/parse-github-url/cli.js" "$@"
else
  exec node  "$basedir/../../../../../parse-github-url@1.0.3/node_modules/parse-github-url/cli.js" "$@"
fi
