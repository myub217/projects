#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/bin/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/bin/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/src/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/build/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules/npm-check-updates/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/npm-check-updates@16.14.20/node_modules:/data/data/com.termux/files/home/projects/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/src/bin/cli.js" "$@"
else
  exec node  "$basedir/../../build/src/bin/cli.js" "$@"
fi
